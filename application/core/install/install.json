    "tables": [
        {
            "tableName": "xy_core_config",
            "tableCreate": [
                "CREATE TABLE `xy_core_config` (",
                "  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '配置ID',",
                "  `module` varchar(255) NOT NULL DEFAULT '' COMMENT '模块名称',",
                "  `configCate` longtext COMMENT '配置分组',",
                "  `name` varchar(32) NOT NULL DEFAULT '' COMMENT '配置名称',",
                "  `title` varchar(32) NOT NULL DEFAULT '' COMMENT '配置标题',",
                "  `configType` longtext COMMENT '配置类型',",
                "  `value` text COMMENT '配置值',",
                "  `defaultValue` text COMMENT '默认值',",
                "  `testValue` text COMMENT '开发模式测试值',",
                "  `placeholder` varchar(100) NOT NULL DEFAULT '' COMMENT '说明',",
                "  `tip` varchar(100) NOT NULL DEFAULT '' COMMENT '配置说明',",
                "  `options` varchar(256) NOT NULL DEFAULT '' COMMENT '配置额外值',",
                "  `isSystem` longtext COMMENT '是否系统配置',",
                "  `isDev` longtext COMMENT '是否开发模式才显示',",
                "  `sortnum` tinyint(4) unsigned NOT NULL DEFAULT '0' COMMENT '排序',",
                "  `status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '状态',",
                "  `deleteTime` longtext COMMENT '删除时间',",
                "  PRIMARY KEY (`id`)",
                ") ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='系统配置表'"
            ],
            "tableRows": []
        },
        {
            "tableName": "xy_core_identity",
            "tableCreate": [
                "CREATE TABLE `xy_core_identity` (",
                "  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'ID',",
                "  `uid` int(11) NOT NULL DEFAULT '0' COMMENT 'UID',",
                "  `identityType` longtext COMMENT '账号类型',",
                "  `identityGroup` longtext COMMENT '账号分组',",
                "  `identifier` varchar(128) NOT NULL DEFAULT '' COMMENT '帐号/openid',",
                "  `verified` tinyint(1) unsigned NOT NULL DEFAULT '0' COMMENT '是否已验证',",
                "  `isOauth2` longtext COMMENT '是否OAuth2登录',",
                "  `createTime` longtext COMMENT '创建时间',",
                "  `deleteTime` longtext COMMENT '删除时间',",
                "  PRIMARY KEY (`id`)",
                ") ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户登陆凭证信息表'"
            ],
            "tableRows": []
        },
        {
            "tableName": "xy_core_login",
            "tableCreate": [
                "CREATE TABLE `xy_core_login` (",
                "  `uid` int(11) NOT NULL DEFAULT '0' COMMENT 'UID',",
                "  `key` varchar(256) NOT NULL DEFAULT '' COMMENT '加密key',",
                "  `token` text NOT NULL COMMENT '登录token',",
                "  `refreshToken` text COMMENT '刷新token',",
                "  `loginTime` longtext COMMENT '登录时间',",
                "  `expireTime` longtext COMMENT '过期时间',",
                "  `clientType` longtext COMMENT '客户端类型1pc2wap3app',",
                "  `clientName` longtext COMMENT '设备名称'",
                ") ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户登录信息表'"
            ],
            "tableRows": []
        },
        {
            "tableName": "xy_core_menu",
            "tableCreate": [
                "CREATE TABLE `xy_core_menu` (",
                "  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',",
                "  `module` varchar(255) NOT NULL COMMENT '模块名称',",
                "  `icon` varchar(64) NOT NULL DEFAULT '' COMMENT '图标',",
                "  `path` varchar(255) NOT NULL COMMENT '路由路径',",
                "  `pmenu` varchar(255) NOT NULL DEFAULT '' COMMENT '父菜单',",
                "  `title` varchar(255) NOT NULL DEFAULT '' COMMENT '菜单标题',",
                "  `tip` varchar(255) NOT NULL DEFAULT '' COMMENT '提示',",
                "  `menuLayer` longtext COMMENT '菜单分层',",
                "  `menuType` longtext COMMENT '菜单类型1导航2按钮3仅接口',",
                "  `routeType` longtext COMMENT '路由类型',",
                "  `apiPrefix` longtext COMMENT '接口前缀',",
                "  `apiSuffix` longtext COMMENT '接口后缀',",
                "  `apiParams` longtext COMMENT '接口参数',",
                "  `apiMethod` longtext COMMENT '接口请求方法',",
                "  `apiExt` longtext COMMENT '接口后缀',",
                "  `doc` longtext COMMENT '接口文档',",
                "  `isHide` longtext COMMENT '是否隐藏',",
                "  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态',",
                "  `sortnum` int(11) NOT NULL DEFAULT '0' COMMENT '排序',",
                "  `deleteTime` longtext COMMENT '删除时间',",
                "  PRIMARY KEY (`id`)",
                ") ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='后台菜单表'"
            ],
            "tableRows": []
        },
